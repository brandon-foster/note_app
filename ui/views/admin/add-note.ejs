<%- include("../inc/begin.ejs") %>
            <div class='row mt-5 mb-5'>
                    <form class="mb-3" action="<%= BASE_DIR %>/admin/add-note" method="POST">
                        <input type="text" name="noteTitle" value="" id="noteTitle" class="form-control" placeholder="title" autofocus>
                        <select name="noteCategory" id="selectInputCategory" class="form-select" aria-label="select a category">
                            <% for (const cat of categoryList) { %>
                            <option value="<%= cat.id %>"><%= cat.text %></option>
                            <% } %>
                        </select>
                        <textarea name="noteBody" value="type here" id="noteBody" class="form-control"></textarea>
                        <input type="submit" value="go" class="btn btn-primary">
                    </form>
<%- include("../inc/end.ejs") %>